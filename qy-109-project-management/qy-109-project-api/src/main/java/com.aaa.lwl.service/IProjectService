package com.aaa.lwl.service;

import com.aaa.lwl.base.ResultData;
import com.aaa.lwl.model.LoginLog;
import com.aaa.lwl.model.User;
import org.springframework.cloud.openfeign.FeignClient;
import org.springframework.web.bind.annotation.PostMapping;
import org.springframework.web.bind.annotation.RequestBody;

/**
 * fileName:IProjectService
 * Author:李守堂
 * createTime:2020/7/15   18:31
 * version:1.0.0
 * Description
 */
@FeignClient(value = "")
public interface IProjectService {
    /**
     * 执行登录操作
     * @param user
     * @return
     */
    @PostMapping("/doLogin")
    ResultData doLogin(@RequestBody User user);


    /**
     * 新增日志
     * @param loginLog
     * @return
     */

    @PostMapping("/addLoginLog")
    Integer addLoginLog(@RequestBody LoginLog loginLog);

}
